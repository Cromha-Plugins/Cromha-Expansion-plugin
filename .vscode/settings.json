{
      "spellright.language": [
            "en-US-10-1."
      ],
      "spellright.documentTypes": [
            "markdown",
            "latex",
            "plaintext",
            "shellscript",
            "txt"
      ],
      "spellright.parserByClass": {
            "txt": {
                  "parser": "plain"
            }
      },
      "cSpell.words": [
            "Distancer",
            "Drak",
            "Hais",
            "Hyperdrive",
            "korath",
            "Lifdot",
            "mereit",
            "Mereti",
            "mineria",
            "Radiothermal",
            "Sestor",
            "Starline",
            "Wel'y"
      ],
      "python.testing.unittestArgs": [
            "-v",
            "-s",
            "./devs",
            "-p",
            "*_test.py"
      ],
      "python.testing.pytestEnabled": false,
      "python.testing.unittestEnabled": true
}